{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Parking\\Parking.Mqtt_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyy.MM.dd HH:mm:ss.fff} | {Level:u4} | {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "ConnectionStrings": {
    "ParkingMqttApiContext": "Server=(localdb)\\mssqllocaldb;Database=ParkingMqttApiContext-eb4b531b-c6b5-498b-bd76-3a70264ee1e1;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}