@page
@model Parking.Mqtt.Api.MqttAdministrationModel
@{
    ViewData["Title"] = "Administration";
}

<h1>Administration</h1>


<div class="row">
    <div class="col-4">
        <table>
            <tr>
                @if (Model.Status.Status.Connected)
                {
                    <td>Connected</td>
                }
                else
                {
                    <td>Disconnected</td>
                }
            </tr>
        </table>
    </div>
    <div class="col-8">

    </div>
</div>
